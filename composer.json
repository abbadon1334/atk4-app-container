{
    "name": "abbadon1334/atk4-app-container",
    "description": "Agile Toolkit Container",
    "license": "MIT",
    "type": "package",
    "keywords": [
        "Container",
        "Agile Toolkit",
        "Atk4"
    ],
    "authors": [
        {
            "name": "Francesco Danti",
            "email": "fdanti@gmail.com"
        }
    ],
    "homepage": "https://github.com/abbadon1334/atk4-container",
    "require": {
        "php": ">=7.4",
        "atk4/core": "dev-develop",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.0",
        "johnkary/phpunit-speedtrap": "^3.3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpunit/phpunit": "^9.5.5",
        "symfony/console": "5.4.14"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Atk4\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atk4\\Container\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "require-release": {
        "php": ">=7.4 <8.2",
        "atk4/core": "~3.1.0",
        "psr/container": "^2.0"
    }
}
